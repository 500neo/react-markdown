{
  "extends": ["bengler", "bengler/react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-var": 0,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0,
    "prefer-template": 0,
    "react/no-unused-prop-types": 0,
    "strict": 0
  }
}
